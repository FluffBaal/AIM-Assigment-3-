{
  "buildCommand": "cd frontend && npm install && npm install terser && npx vite build",
  "outputDirectory": "frontend/dist",
  "framework": null,
  "functions": {
    "api/index.py": {
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/((?!assets/|vite\.svg).*)",
      "destination": "/index.html"
    }
  ]
}